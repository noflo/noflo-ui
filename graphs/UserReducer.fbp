INPORT=Dispatch.IN:IN
OUTPORT=MergeState.OUT:STATE

'info,error' -> ROUTES Dispatch(routers/GroupRouter)

# Set latest user info to state
'user' -> KEY SetUser(ui/SetStateValue)
Dispatch OUT[0] -> IN SetUser OUT -> IN MergeState(core/Merge)

# Set error to state
Dispatch OUT[1] -> ERROR ErrorToCtx(ui/ErrorToContext)
ErrorToCtx OUT -> IN MergeState
